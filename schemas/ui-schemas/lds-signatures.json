{
	"$validator": "/dcic/signature-commons-schema/v6/core/schema.json",
	"id": "90f102a1-f68d-4b5b-b8c9-d7235738dfaa",
	"meta": {
		"match": {
			"${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/signature.json",
			"${library.dataset}": "LDS*",
			"${library.dataset_type}": "rank_matrix"
		},
		"$validator": "/dcic/signature-commons-schema/v6/meta/schema/ui-schema.json",
		"properties": {
			"Image": {
				"alt": "${library.meta.description}",
				"src": "${PREFIX}/${library.meta.icon}",
				"type": "img",
				"priority": 1,
				"visibility": 1
			},
			"Signature ID": {
				"text": "${meta.lincs_sig_id}",
				"type": "title",
				"priority": 1,
				"visibility": 1
			},
			"Dataset": {
				"text": "${library.meta.description}",
				"type": "subtitle",
				"priority": 1,
				"visibility": 1
			},
			"Download": {
				"icon": "mdi-download",
				"text": "Download",
				"type": "component",
				"props": {
					"data": {
						"type": "prop-array",
						"items": [
							{
								"type": "prop-object",
								"properties": {
									"text": {
										"text": "Download",
										"type": "text"
									},
									"icon": {
										"text": "mdi-download",
										"type": "text"
									},
									"url": {
										"text": "${meta.file_url}",
										"type": "text"
									}
								}
							}
						]
					}
				},
				"location": "chip",
				"priority": 10,
				"component": "download",
				"visibility": 1
			},
			"Cell-line": {
				"text": "${meta.cell_line.cell_id}",
				"type": "text",
				"icon": "mdi-microscope",
				"field": "meta.cell_line.cell_id",
				"priority": 4,
				"visibility": 1
			},
			"Time Point": {
				"text": "${meta.pert.pert_itime}",
				"type": "text",
				"icon": "mdi-av-timer",
				"field": "meta.pert.pert_itime",
				"priority": 3,
				"visibility": 1
			},
			"Perturbation": {
				"text": "${meta.pert.pert_iname}",
				"field": "meta.pert.pert_iname",
				"type": "text",
				"icon": "mdi-hexagon-multiple",
				"priority": 2,
				"visibility": 1
			},
			"Concentration": {
				"text": "${meta.pert.pert_idose}",
				"field": "meta.pert.pert_idose",
				"type": "text",
				"icon": "mdi-eyedropper",
				"priority": 3,
				"visibility": 1
			},
			"Direction": {
				"text": "${direction}",
				"type": "text",
				"priority": 4,
				"condition": {
					"${meta['logp-fisher']}": {
						"ne": "undefined"
					}
				},
				"visibility": 1
			},
			"Log(p) Fisher": {
				"text": "${Number.parseFloat(scores['logp-fisher']).toPrecision(4)}",
				"type": "text",
				"priority": 5
			},
			"Log(p) Ave": {
				"text": "${Number.parseFloat(scores['logp-avg']).toPrecision(4)}",
				"type": "text",
				"priority": 5
			}
		}
	}
}