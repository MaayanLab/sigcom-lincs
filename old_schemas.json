[
	{
		"$validator": "/dcic/signature-commons-schema/v5/core/schema.json",
		"id": "90f102a1-f68d-4b5b-b8c9-d7235738dfaa",
		"meta": {
			"match": {
				"${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/signature.json",
				"${library.dataset}": "LDS*",
				"${library.dataset_type}": "rank_matrix"
			},
			"$validator": "https://raw.githubusercontent.com/dcic/signature-commons-schema/v5/meta/schema/ui-schema.json",
			"properties": {
				"P-Up": {
					"text": "${meta['p-up']}",
					"type": "text",
					"priority": 5,
					"condition": {
						"${meta['p-up']}": {
							"ne": null
						}
					},
					"columnType": "number",
					"visibility": 1
				},
				"Z-Up": {
					"text": "${meta['z-up']}",
					"type": "text",
					"priority": 5,
					"condition": {
						"${meta['z-up']}": {
							"ne": null
						}
					},
					"columnType": "number",
					"visibility": 1
				},
				"Image": {
					"alt": "${library.meta.title}",
					"src": "${PREFIX}/${library.meta.creators[0].affiliations[0].extraProperties[0].values[0]}",
					"icon": true,
					"text": "${library.meta.title}",
					"type": "img",
					"priority": 1,
					"subtitle": true,
					"visibility": 1
				},
				"P-Down": {
					"text": "${meta['p-down']}",
					"type": "text",
					"priority": 5,
					"condition": {
						"${meta['p-down']}": {
							"ne": null
						}
					},
					"columnType": "number",
					"visibility": 1
				},
				"Z-Down": {
					"text": "${meta['z-down']}",
					"type": "text",
					"priority": 5,
					"condition": {
						"${meta['z-down']}": {
							"ne": null
						}
					},
					"columnType": "number",
					"visibility": 1
				},
				"P-Value": {
					"text": "${meta['p-value']}",
					"type": "text",
					"priority": 5,
					"condition": {
						"${meta['p-value']}": {
							"ne": null
						}
					},
					"columnType": "number",
					"visibility": 1,
					"columnOptions": {
						"sort": true,
						"sortDirection": "asc"
					}
				},
				"Download": {
					"text": "Download",
					"type": "text",
					"MDI_Icon": "mdi-download",
					"download": true,
					"priority": 10,
					"hyperlink": "${meta.file_url}",
					"visibility": 1
				},
				"Cell-line": {
					"text": "${meta.cell_line.cell_id}",
					"type": "text",
					"MDI_Icon": "mdi-microscope",
					"priority": 4,
					"clickable": true,
					"visibility": 1
				},
				"Direction": {
					"text": "${meta['direction'] === 1 ? 'up' : 'down'}",
					"type": "text",
					"priority": 4,
					"condition": {
						"${meta['logp-fisher']}": {
							"ne": "undefined"
						}
					},
					"visibility": 1
				},
				"Odds Ratio": {
					"text": "${meta.oddsratio}",
					"type": "text",
					"priority": 5,
					"condition": {
						"${meta.oddsratio}": {
							"ne": null
						}
					},
					"columnType": "number",
					"visibility": 1
				},
				"Time Point": {
					"text": "${meta.pert.pert_itime}",
					"type": "text",
					"MDI_Icon": "mdi-av-timer",
					"priority": 3,
					"clickable": false,
					"visibility": 1
				},
				"Perturbation": {
					"text": "${meta.pert.pert_iname}",
					"type": "text",
					"MDI_Icon": "mdi-hexagon-multiple",
					"priority": 2,
					"clickable": true,
					"visibility": 1
				},
				"Signature ID": {
					"name": true,
					"text": "${meta.lincs_sig_id}",
					"type": "text",
					"priority": 1,
					"visibility": 1
				},
				"Concentration": {
					"text": "${meta.pert.pert_idose}",
					"type": "text",
					"MDI_Icon": "mdi-eyedropper",
					"priority": 3,
					"clickable": false,
					"visibility": 1
				},
				"Gene Set Size": {
					"text": "${meta.setsize}",
					"type": "text",
					"priority": 5,
					"condition": {
						"${meta.setsize}": {
							"ne": null
						}
					},
					"columnType": "number",
					"visibility": 1
				},
				"Log(p) Fisher": {
					"text": "${meta['logp-fisher']}",
					"type": "text",
					"priority": 5,
					"condition": {
						"${meta['logp-fisher']}": {
							"ne": null
						}
					},
					"columnType": "number",
					"visibility": 1,
					"columnOptions": {
						"sort": true,
						"sortDirection": "desc"
					}
				},
				"Log(p) Average": {
					"text": "${meta['logp-avg']}",
					"type": "text",
					"priority": 5,
					"condition": {
						"${meta['logp-avg']}": {
							"ne": null
						}
					},
					"columnType": "number",
					"visibility": 1
				}
			}
		}
	},
